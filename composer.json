{
    "name": "dgaitan/serializable",
    "description": "Simple Seriazable Utilities for Laravel Models",
    "type": "library",
    "license": "Public",
    "autoload": {
        "psr-4": {
            "Dgaitan\\Serializable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgaitan\\Serializable\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Gaitan",
            "email": "jdavid.gaitan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
      "mockery/mockery": "^1.0",
      "orchestra/testbench": "^7.31|^8.11|^9.0",
      "php-http/guzzle7-adapter": "^1.0",
      "phpstan/phpstan": "^1.10",
      "phpunit/phpunit": "^9.3|^10.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
